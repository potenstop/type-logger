{
  "appenders": [
    {
      "name": "CONSOLE",
      "class": "ConsoleAppender",
      "layout": {
        "class": "SimpleLayout"
      }
    }, {
      "name": "FORMAT_CONSOLE",
      "class": "ConsoleAppender",
      "layout": {
        "class": "PatternLayout",
        "pattern": "%date{yyyy-MM-dd HH:mm:ss.S} [%level] [%class.%method] %line %row - %msg%n"
      }
    }

  ],
  "root": {
    "level": "TRACE",
    "appenderRefs": [{
      "ref": "CONSOLE"
    }, {
      "ref": "FORMAT_CONSOLE"
    }]
  }
}
